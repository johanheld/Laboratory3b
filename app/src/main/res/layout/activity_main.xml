<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    tools:context="com.example.johan.laboratory3b.MainActivity">

    <fragment
        android:id="@+id/listFragment"
        android:name="com.example.johan.laboratory3b.ListFragment"
        android:layout_width="394dp"
        android:layout_height="212dp"
        tools:layout_editor_absoluteX="-1dp"
        tools:layout_editor_absoluteY="0dp"
        tools:layout="@layout/fragment_list"/>

    <fragment
        android:id="@+id/contentFragment"
        android:name="com.example.johan.laboratory3b.InfoFragment"
        android:layout_width="386dp"
        android:layout_height="298dp"
        android:layout_marginTop="0dp"
        app:layout_constraintTop_toBottomOf="@+id/listFragment"
        tools:layout="@layout/fragment_info"
        tools:layout_editor_absoluteX="0dp"/>
</android.support.constraint.ConstraintLayout>
